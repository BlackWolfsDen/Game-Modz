<configs>
    <!-- higher amp solar panels -->
	<set xpath="/blocks/block[@name='solarbank']/property[@name='OutputPerStack']/@value">220</set><!-- Max possible watts per cell -->
	<set xpath="/blocks/block[@name='solarbank']/property[@name='MaxPower']/@value">1320</set>

    <!-- better generators -->
 	<set xpath="/blocks/block[@name='generatorbank']/property[@name='MaxFuel']/@value">7500</set>
 	<set xpath="/blocks/block[@name='generatorbank']/property[@name='OutputPerFuel']/@value">11250</set>
 	<set xpath="/blocks/block[@name='generatorbank']/property[@name='OutputPerStack']/@value">2500</set>
 	<set xpath="/blocks/block[@name='generatorbank']/property[@name='MaxPower']/@value">24500</set> <!-- 12250 = 300w :: 24500 = 600w :: 36750 = 900w-->

    <!-- higher amp deep cycle battery bank -->
 	<set xpath="/blocks/block[@name='batterybank']/property[@name='MaxPower']/@value">1200</set>
 	<set xpath="/blocks/block[@name='batterybank']/property[@name='InputPerTick']/@value">7</set>
 	<set xpath="/blocks/block[@name='batterybank']/property[@name='ChargePerInput']/@value">2</set>
 	<set xpath="/blocks/block[@name='batterybank']/property[@name='OutputPerStack']/@value">200</set>
 	<set xpath="/blocks/block[@name='batterybank']/property[@name='OutputPerCharge']/@value">180</set>

</configs>
